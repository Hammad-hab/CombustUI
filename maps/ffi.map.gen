        },
        "returns": "c_void"
    },

    "mjuiSetWindowTitle": {
        "@redef": "mjuiSetWidgetLabel"
    },

    "mjuiWindowVisibilityStatus": {
        "arguments": {
            "window": "FLTK_WIDGET_POINTER",
            "condition": "Int"
        },
        "returns": "Int"
    },

    "mjuiEventKey": {
        "arguments": {},
        "returns": "Int"
    }
}