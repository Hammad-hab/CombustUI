        "templates.map.gen",
        "layouts.map.gen"
    ],
    
    "@concat_variations": {
        "target": "mjuiSetWidgetId",
        "variations": [
            "_button"
        ]
    },

    "mjuiSetWidgetId":
    {
        "@cbindto": "set_id",
        "arguments": {
            "widget": "FLTK_WIDGET_POINTER",
            "id": "Int"
        },
        "returns": "c_void"
    },

     "mjuiWindowSetResizable":
    {
        "arguments": {
            "window": "FLTK_WIDGET_POINTER",
            "widgit": "FLTK_WIDGET_POINTER"
        },
        "returns": "c_void"
    },

    "mjuiCheckButtonGetState": {
        "arguments": {
            "checkButton": "FLTK_WIDGET_POINTER"
        },
        "returns": "Int"
    },
    "mjuiLoadImage": {
        "arguments": {
            "w": "Int",
            "h": "Int",
            "type": "Int",
            "path": "StringBytes"
        },
        "returns": "Int32",
        "@cbindto": "load_img"
    },

        "mjuiApplyImage": {
        "returns": "c_void",
        "arguments": {
            "widget": "FLTK_WIDGET_POINTER",
            "img": "Int32"
        }
    },
 "mjuiFlexCalculateLayout":
    {
        "arguments": {
            "layout": "FLTK_WIDGET_POINTER"
        },
        "returns": "c_void"
    },

    "mjuiSetFlexResize":
    {
        "arguments": {
            "layout": "FLTK_WIDGET_POINTER",
            "direction": "Int"
        },
        "returns": "c_void"
    },

    "fl_ready": {
        "arguments": {}, 
        "returns": "Int32",
        "@cbindto": "fltk_check"
    },

    "fl_check": {
        "arguments": {}, 
        "returns": "Int32",
        "@cbindto": "fltk_ready"
    }, 

    "mjuiShowWidget": {
        "@cbindto": "show_widget",
        "arguments": {
            "widget": "FLTK_WIDGET_POINTER"
        },
        "returns": "c_void"
    },
    "mjuiSetWidgetColor": {
        "@cbindto": "set_widget_color",
        "arguments": {
            "widget": "FLTK_WIDGET_POINTER",
            "colour": "UInt32"
        },
        "returns": "c_void"
    },

    "mjuiWidgetTextColor": {
        "arguments": {
            "widget": "FLTK_WIDGET_POINTER",
            "colour": "UInt32"
        },
        "returns": "c_void",
        "@cbindto": "set_text_color"
    },

    "mjuiSetSelectionColor": {
        "arguments": {
            "widget": "FLTK_WIDGET_POINTER",
            "colour": "UInt32"
        },
        "returns": "c_void",
        "@cbindto": "set_widget_color2"
    },
    
    "grab_fltk_event": {
        "arguments": {},
        "returns": "Int",
        "@cbindto": "listEvents"
    },

    "mjuiWindowPositionSet": {
        "arguments": {
            "window": "FLTK_WIDGET_POINTER",
            "x": "Int",
            "y": "Int"
        },
        "returns": "c_void",
        "@cbindto": "mjuiWindowPositionSet"
    },

    "mjuiSpawnWindow": {
        "arguments": {
            "w": "Int32",
            "h": "Int32",
            "resize": "Int",
            "fullscreen": "Int",
            "no_border": "Int",
            "label": "StringBytes"
        },
        "returns": "FLTK_WIDGET_POINTER",
        "@cbindto": "mjuiCreateWindow"
    },


    "mjuiCreateLabel": {
        "@redef": "@basicWidgetPointerFunction",
        "returns": "FLTK_WIDGET_POINTER"
    },

    "mjuiCreateButton": {
        "@redef": "@basicIDWidgetPointerFunction",
        "returns": "FLTK_WIDGET_POINTER"
    },

    "mjuiCreateCheckButton": {
        "@redef": "@basicIDWidgetPointerFunction",
        "returns": "FLTK_WIDGET_POINTER"
    },

    "mjuiCreateMultilineInput": {
        "@redef": "@basicIDWidgetPointerFunction",
        "returns": "FLTK_WIDGET_POINTER"
    },

    "mjuiCreateInput": {
        "@redef": "@basicIDWidgetPointerFunction",
        "returns": "FLTK_WIDGET_POINTER"
    },

    "mjuiGrabInput": {
        "arguments": {
            "widget": "FLTK_WIDGET_POINTER"
        },
        "returns": "StringBytes"
    },

    "mjuiSetWidgetBox": {
        "arguments": {
            "widget": "FLTK_WIDGET_POINTER",
